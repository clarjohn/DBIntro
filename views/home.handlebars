<h1>Enter your exercise here</h1>
<form action="/" method="post" id = "inpexc">
    <div style="float:left;margin-right:20px;">
    <lable>Name:</lable><br/>
        <input type="text" id = "name_Input" name = "name" size = "30" maxlenght = "100">    
    </div>
    <div style="float:left;margin-right:20px;">
        <lable>Reps:</lable><br/>
        <input type="text" id = "reps_Input" name = "reps" size = "30" maxlenght = "100">    
    </div>
    <div style="float:left;margin-right:20px;">
        <lable>Weight:</lable><br/>
        <input type="number" id = "weight_Input" name = "weight" size = "30" maxlenght = "100">
    </div>
    <div style="float:left;margin-right:20px;">
        <lable>Date:</lable><br/>
        <input type="date" id = "date_Input" name = "date" size = "30" maxlenght = "100">
    </div>   
    <br/>
    <input type="submit" id = "Load_ID" name="command" value="enter">
    
</form>

